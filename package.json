{
  "name": "asr-pharma-backend",
  "version": "1.0.0",
  "description": "Pharmacy Management System Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "migrate": "sequelize-cli db:migrate --env production --config src/config/config.js",
    "seed": "sequelize-cli db:seed:all --env production --config src/config/config.js",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docs": "jsdoc src/ -d docs/api"
  },
  "keywords": [
    "pharmacy",
    "management",
    "nodejs",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "ASR Pharma",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^1.7.0",
    "redis": "^4.7.1",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "twilio": "^5.8.0",
    "uuid": "^11.1.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
